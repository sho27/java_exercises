/* Coding Exercise 28 - Is there a greater element */

public class ArrayMagic {
    
 public boolean doesHaveElementGreaterThan(int[] array, int number) {
    	if(array.length == 0) {
    		return false;
    	}
    	
    	for(int num: array) {
    		if(num > number) {
    			return true;
    		}
    	}
    	return false;
    }
}
