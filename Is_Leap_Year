/*Coding E+xercise 8 - Is Leap year*/

public class LeapYearChecker {
    public boolean isLeapYear(int year) {
        
        if (year <= 0) {
			   return false;
		   }

	   if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)) {
	            return true;
	        }
       return false;
    }
}
